{
  "name": "tarasov-krk/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tarasov Andrey",
      "email": "tarasov.krk@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": ">=7.0.0",
    "minime/annotations": "2.3.1",
    "ronasit/laravel-helpers": "1.3.*",
    "ronasit/local-data-collector": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\AutoDoc\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\AutoDoc\\AutoDocServiceProvider",
        "RonasIT\\Support\\DataCollectors\\LocalDataCollectorServiceProvider"
      ]
    }
  }
}
